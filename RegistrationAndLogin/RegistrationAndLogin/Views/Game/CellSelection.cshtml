@model RegistrationAndLogin.Models.GameBoard


<h2>Welcome to the Game Cell Selection page</h2>
<h4>Valid selection are numbers between 1 and 10</h4>
@using (Html.BeginForm("GameDisplay", "Game"))
{
    @Html.TextBoxFor(m => m.CellSelection.Row)<span> : Row</span>
    <br />
    @Html.TextBoxFor(m => m.CellSelection.Column)<span> : Column</span>
    <br />
    <input type="submit" value="Take Turn" />
}